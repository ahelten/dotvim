[core]
        editor = vim
#       fileMode = false

[alias]
        lol = log --graph --decorate --pretty=oneline --abbrev-commit --all
        logtag = log --date-order --graph --tags --simplify-by-decoration --pretty=format:'%ai %h %<(30) %d %s' --abbrev-commit
        diffw = -c core.fileMode=false diff -w
        diffm = -c core.fileMode=false diff -w --diff-filter=M

[color]
        diff = auto
        status = auto
        branch = auto
        interactive = auto
        ui = true
        pager = true

[push]
        default = matching

