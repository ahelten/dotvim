[user]
        email =
        name =
[core]
        editor = vim
        autocrlf = input
#       fileMode = false

[merge]
        conflictstyle = diff3

[alias]
        lol = log --graph --pretty=format:'%C(auto)%h %C(green)%ad %C(blue)%an %C(yellow)%d %C(reset)%s' --date=short --all
        lolold = log --graph --decorate --pretty=oneline --abbrev-commit --all
        logtag = log --date-order --graph --tags --simplify-by-decoration --pretty=format:'%ai %h %<(30) %d %s' --abbrev-commit
        diffw = -c core.fileMode=false diff -w
        diffm = -c core.fileMode=false diff -w --diff-filter=M

[color]
        diff = auto
        status = auto
        branch = auto
        interactive = auto
        ui = true
        pager = true

[push]
        default = matching

[url "ssh://git@github.com/"]
        insteadOf = https://github.com/

